<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentTransitions">true</item>
    </style>

    <style name="TapToolbarTitle">
        <item name="android:ellipsize">end</item>
        <item name="android:fontFamily">@font/tap_font_pt_root_bold</item>
        <item name="android:gravity">center</item>
        <item name="android:letterSpacing">-0.02</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/tap_black_44</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="TapTitle">
        <item name="android:fontFamily">@font/tap_font_pt_root_bold</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:textColor">@color/tap_black_19</item>
        <item name="android:textSize">40sp</item>
    </style>

    <style name="TapPageTitle">
        <item name="android:ellipsize">end</item>
        <item name="android:fontFamily">@font/tap_font_pt_root_bold</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/tap_black_19</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="TapFormFieldName">
        <item name="android:fontFamily">@font/tap_font_pt_root_medium</item>
        <item name="android:letterSpacing">-0.03</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:textColor">@color/tap_black_19</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="TapTextButtonOrange">
        <item name="android:fontFamily">@font/tap_font_pt_root_bold</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:textColor">@color/tap_pumpkin_orange</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="TapFormFieldBody">
        <item name="android:fontFamily">@font/tap_font_pt_root_regular</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/tap_black_19</item>
        <item name="android:textColorHint">@color/tap_grey_9b</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="TapFormGuide">
        <item name="android:fontFamily">@font/tap_font_pt_root_regular</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:textColor">@color/tap_black_19</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="TapFormError">
        <item name="android:fontFamily">@font/tap_font_pt_root_regular</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:textColor">@color/tap_watermelon_red</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="TapRoomName">
        <item name="android:ellipsize">end</item>
        <item name="android:fontFamily">@font/tap_font_pt_root_bold</item>
        <item name="android:gravity">center</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/tap_black_44</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="TapImageDetailSender">
        <item name="android:ellipsize">end</item>
        <item name="android:fontFamily">@font/tap_font_pt_root_bold</item>
        <item name="android:gravity">start</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/tap_white</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="TapButton">
        <item name="android:background">@drawable/tap_bg_mango_pumpkin_stroke_pumpkin_1dp_rounded_6dp_ripple</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:fontFamily">@font/tap_font_pt_root_bold</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/tap_white</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="TapButtonText">
        <item name="android:fontFamily">@font/tap_font_pt_root_bold</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:textColor">@color/tap_white</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="TapSearchBar">
        <item name="android:background">@color/tap_transparent_white</item>
        <item name="android:fontFamily">@font/tap_font_pt_root_regular</item>
        <item name="android:imeOptions">actionSearch</item>
        <item name="android:inputType">text</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/tap_black_44</item>
        <item name="android:textColorHint">@color/tap_grey_9b</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="TapSectionLabel">
        <item name="android:fontFamily">@font/tap_font_pt_root_bold</item>
        <item name="android:letterSpacing">0.09</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textColor">@color/tap_black_19</item>
        <item name="android:textSize">11sp</item>
    </style>

    <style name="TapContactName">
        <item name="android:ellipsize">end</item>
        <item name="android:fontFamily">@font/tap_font_pt_root_bold</item>
        <item name="android:gravity">start</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/tap_black_44</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="TapRoomMessage">
        <item name="android:ellipsize">end</item>
        <item name="android:fontFamily">@font/tap_font_pt_root_regular</item>
        <item name="android:gravity">start</item>
        <item name="android:textColor">@color/tap_grey_9b</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="TapImageDetailStatus">
        <item name="android:ellipsize">end</item>
        <item name="android:fontFamily">@font/tap_font_pt_root_regular</item>
        <item name="android:gravity">start</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">@color/tap_white</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="TapMessageStatus">
        <item name="android:fontFamily">@font/tap_font_pt_root_regular</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/tap_grey_9b</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="TapMenuLabel">
        <item name="android:fontFamily">@font/tap_font_pt_root_regular</item>
        <item name="android:letterSpacing">-0.02</item>
        <item name="android:textColor">@color/tap_black_44</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="TapMessageBody">
        <item name="android:fontFamily">@font/tap_font_pt_root_regular</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="TapMessageUserName">
        <item name="android:fontFamily">@font/tap_font_pt_root_bold</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="TapMessageLog">
        <item name="android:fontFamily">@font/tap_font_pt_root_bold</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:textColor">@color/tap_grey_9b</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="TapQuoteTitle">
        <item name="android:fontFamily">@font/tap_font_pt_root_bold</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">@color/tap_black_44</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="TapQuoteBody">
        <item name="android:fontFamily">@font/tap_font_pt_root_regular</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">@color/tap_black_44</item>
        <item name="android:textSize">12sp</item>
        <item name="android:lineSpacingExtra">1sp</item>
    </style>

    <style name="TapUnreadBadge">
        <item name="android:background">@drawable/tap_bg_mango_pumpkin_stroke_pumpkin_1dp_rounded_10dp</item>
        <item name="android:fontFamily">@font/tap_font_pt_root_bold</item>
        <item name="android:paddingStart">7dp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingEnd">7dp</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">@color/tap_white</item>
        <item name="android:textSize">12dp</item>
    </style>

    <style name="TapProductDetailsLabel">
        <item name="android:fontFamily">@font/tap_font_pt_root_bold</item>
        <item name="android:textColor">@color/tap_black_44</item>
        <item name="android:textSize">12sp</item>
        <item name="android:letterSpacing">-0.02</item>
    </style>

    <style name="TapProductDetail">
        <item name="android:ellipsize">end</item>
        <item name="android:fontFamily">@font/tap_font_pt_root_medium</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/tap_purply</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="TapProductDescription">
        <item name="android:fontFamily">@font/tap_font_pt_root_regular</item>
        <item name="android:ellipsize">end</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">@color/tap_black_44</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="TapLoadingText">
        <item name="android:ellipsize">end</item>
        <item name="android:fontFamily">@font/tap_font_pt_root_medium</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:maxLines">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/tap_greenBlue</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="TapErrorMessageTitle">
        <item name="android:ellipsize">end</item>
        <item name="android:fontFamily">@font/tap_font_pt_root_medium</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:maxLines">1</item>
        <item name="android:textColor">@color/tap_coral_pink</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="TapErrorMessageBody">
        <item name="android:fontFamily">@font/tap_font_pt_root_regular</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:textColor">@color/tap_coral_pink</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="SwipeBackLayout">
        <item name="edge_size">50dip</item>
        <!--<item name="tap_shadow_left">@drawable/tap_shadow_left</item>-->
        <!--<item name="tap_shadow_right">@drawable/tap_shadow_right</item>-->
        <!--<item name="tap_shadow_bottom">@drawable/tap_shadow_bottom</item>-->
    </style>

    <style name="EditTextTheme">
        <item name="colorControlActivated">@color/tap_white</item>
    </style>

    <style name="TapMyProgressBarNoPadding" parent="@style/Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:progressTint">@color/tap_white</item>
        <item name="android:progressBackgroundTint">@color/tap_purply_two</item>
        <item name="android:padding">0dp</item>
    </style>

    <style name="TapSwitchColor">
        <!-- Active thumb color & Active track color(30% transparency) -->
        <item name="colorControlActivated">@color/tap_pumpkin_orange</item>
        <!-- Inactive thumb color -->
        <item name="colorSwitchThumbNormal">@color/tap_grey_9b</item>
        <!-- Inactive track color(30% transparency) -->
        <item name="android:colorForeground">@color/tap_grey_9b</item>
    </style>

</resources>
